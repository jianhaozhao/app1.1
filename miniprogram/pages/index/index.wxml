<!--index.wxml-->
<view class='top'></view>

<view class="container">
  <view class='main'>
    <view wx:for="{{sort}}" wx:key='*this' >
      <view class='btn' data-value='{{index}}' catch:tap='bindViewTap'>
        <view class='btn-icon'>
          <image src='{{item.url}}'></image>
        </view>
        <text>{{item.name}}</text>
      </view>
    </view>
  </view>
</view>
